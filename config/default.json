{
    "name": "default",
    "model": {
        "which_model": {
            "name": [
                "models.model",
                "Model"
            ],
            "args": {}
        },
        "networks": [
            {
                "name": [
                    "models.cdan",
                    "CDAN"
                ],
                "args": {}
            }
        ]
    },
    "train": {
        "device": "cuda",
        "n_epoch": 80,
        "lr": 1e-3,
        "dataset": {
            "name": [
                "data.dataset",
                "PairedDataset"
            ],
            "args": {
                "low_light_root": "../LOLdataset/train/low", 
                "normal_light_root": "../LOLdataset/train/high",
                "image_size": [600, 400]
            }
        },
        "dataloader": {
            "args": {
                "batch_size": 16,
                "shuffle": "False",
                "num_workers": 4
            }
        },
        "model_path": "saved_models/",
        "model_name": "CDAN.pt"
    },
    "test": {
        "device": "cuda",
        "dataset": {
            "name": [
                "data.dataset",
                "PairedDataset"
            ],
            "args": {
                "low_light_root": "../LOLdataset/test/low", 
                "normal_light_root": "../LOLdataset/test/high",
                "image_size": [600, 400]
            },
            "is_paired": "True"
        },
        "dataloader": {
            "args": {
                "batch_size": 16,
                "shuffle": "False",
                "num_workers": 4
            }
        },
        "apply_post_processing": "True",
        "model_path": "saved_models/",
        "model_name": "CDAN.pt",
        "output_images_path": "output_images/"
    }
}